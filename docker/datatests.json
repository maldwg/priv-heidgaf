          "queryType": "table",
          "rawSql": "SELECT count(detected_ips.client_ip) AS number_of_detected_ips\nFROM (\n  SELECT DISTINCT client_ip\n  FROM alerts\n) AS detected_ips\n",
          "refId": "number_of_detected_ips"
        },
        {
          "datasource": {
            "type": "grafana-clickhouse-datasource",
            "uid": "PDEE91DDB90597936"
          },
          "editorType": "sql",
          "format": 1,
          "hide": true,
          "meta": {
            "builderOptions": {
              "columns": [],
