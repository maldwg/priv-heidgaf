              "queryType": "table",
              "table": ""
            }
          },
          "pluginVersion": "4.5.1",
          "queryType": "table",
          "rawSql": "SELECT count(malicious_ips.client_ip) AS number_of_malicious_ips\nFROM (\n  SELECT DISTINCT client_ip\n  FROM (\n    SELECT *\n    FROM (\n      SELECT client_ip, arrayJoin(domain_list) AS domain_name\n      FROM (\n        SELECT DISTINCT client_ip, JSONExtract(domain_names, 'Array(String)') AS domain_list\n        FROM alerts\n      )\n    ) AS detected_ips_and_their_requested_domains\n    INNER JOIN (\n      SELECT query\n      FROM dgta_dataset\n      WHERE class = 1\n    ) AS malicious_queries ON malicious_queries.query = detected_ips_and_their_requested_domains.domain_name\n  )\n) AS malicious_ips",
          "refId": "number_of_malicious_ips"
        },
        {
          "datasource": {
            "name": "Expression",
            "type": "__expr__",
            "uid": "__expr__"
          },
          "expression": "$number_of_detected_ips / $number_of_malicious_ips",
          "hide": false,
          "refId": "A",
          "type": "math"
        }
      ],
      "title": "Malicious Domains / All Domains",
      "transparent": true,
      "type": "stat"
